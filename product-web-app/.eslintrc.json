{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "airbnb-typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "react/jsx-filename-extension": "off",
        "class-methods-use-this": "off",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "class-methods-use-this": "off"
    },
    "overrides": [
        {
            "files": ["*.test.ts", "*.spec.ts"],
            "rules": {
                "@typescript-eslint/no-unused-expressions": "off"
          }
        }
    ]
}



